{
	"name": "drill",
	"authors": [
		"Federico Santamorena"
	],
	"version": "1.0.0",
	"description": "Search files without indexing, but clever crawling.",
	"copyright": "Copyright Â© 2019, Federico Santamorena",
	"license": "GPL-2.0",
	"homepage": "https://github.com/yatima1460/Drill",
	"preGenerateCommands-posix": [
		"echo $$DUB_PACKAGE_VERSION > DRILL_VERSION",
		"git rev-parse HEAD >> DRILL_VERSION",
		"echo $$DUB_CONFIG >> DRILL_VERSION",
		"echo $$DUB_ARCH >> DRILL_VERSION",
		"echo $$DUB_BUILD_TYPE >> DRILL_VERSION",
		"echo $$DUB_PLATFORM >> DRILL_VERSION",
		"echo $$DUB_BUILD_MODE >> DRILL_VERSION",
		"echo $$DC_BASE >> DRILL_VERSION",
		"echo -n $$D_FRONTEND_VER >> DRILL_VERSION"
	],
	"preGenerateCommands-windows": [],
	"targetName": "DrillExecutable",
	"targetType": "executable",
	"configurations": [
		{
			"name": "CLI",
			"sourceFiles": [
				"drill/core/FileInfo.d",
				"drill/core/Core.d",
				"drill/core/Crawler.d",
				"drill/core/Utils.d",
				"drill/cli/Cli.d"
			],
			"dependencies": {
				"datefmt": {
					"path": "vendor/datefmt"
				}
			}
		},
		{
			"name": "GTK",
			"sourceFiles": [
				"drill/core/FileInfo.d",
				"drill/core/Core.d",
				"drill/core/Crawler.d",
				"drill/core/Utils.d",
				"drill/gtkui/Window.d",
				"drill/gtkui/Main.d"
			],
			"dependencies": {
				"gtk-d:gtkd": {
					"path": "vendor/GtkD"
				},
				"datefmt": {
					"path": "vendor/datefmt"
				}
			}
		}
	]
}